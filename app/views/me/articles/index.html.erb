<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %>
      <ul>
  		<li><%= link_to "Edit", edit_me_article_path(article) %>
  		<%= link_to "Show", me_article_path(article) %>
  		<%= link_to "Delete", me_article_path(article), method: :delete, data: {confirm: "Are you sure?"} %>
  			
  		</li>
	  </ul>
    </li>
  <% end %>
</ul>
